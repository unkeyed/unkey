{
  "$schema": "schema.json",
  "port": "8080",
  "rpcPort": "9095",
  "pprof": {
    "username": "admin",
    "password": "admin"
  },
  "nodeId": "node_local",
  "logging": {},
  "cluster": {
    "authToken": "agent-auth-secret",
    "serfAddr": "localhost:9090",
    "rpcAddr": "localhost:9095",
    "join": {
      "env": {
        "addrs": [
          "unkey-agent-1:9090"
        ]
      }
    }
  },
  "services": {
    "ratelimit": {
      "authToken": "agent-auth-secret"
    },
    "eventRouter": {
      "authToken": "agent-auth-secret",
      "tinybird": {
        "token": "fake-token",
        "batchSize": 1000,
        "flushInterval": 1,
        "bufferSize": 10000
      }
    }
  },
  "prometheus": {
    "path": "/metrics",
    "port": 2112
  }
}
