{
  "$schema": "schema.json",
  "port": "${PORT}",
  "rpcPort": "${RPC_PORT}",
  "pprof": {
    "username": "admin",
    "password": "admin"
  },
  "authToken": "${AUTH_TOKEN}",
  "nodeId": "${NODE_ID}",
  "logging": {},
  "services": {
    "vault": {
      "s3Url": "${VAULT_S3_URL}",
      "s3Bucket": "${VAULT_S3_BUCKET}",
      "s3AccessKeyId": "${VAULT_S3_ACCESS_KEY_ID}",
      "s3AccessKeySecret": "${VAULT_S3_ACCESS_KEY_SECRET}",
      "masterKeys": "${VAULT_MASTER_KEYS}"
    }
  },
  "prometheus": {
    "path": "/metrics",
    "port": 2112
  },
  "clickhouse": {
    "url": "${CLICKHOUSE_URL}"
  }
}
