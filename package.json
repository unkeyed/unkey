{
  "name": "unkey",
  "workspaces": ["apps/*", "packages/*"],
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "local": "pnpm --dir=tools/local local",
    "dev": "turbo run dev",
    "build": "pnpm turbo run build",
    "fmt": "pnpm biome format . --write && pnpm biome check . --apply",
    "test": "turbo run test",
    "bootstrap": "turbo run bootstrap",
    "commit": "cz",
    "bump-versions": "pnpm changeset version && pnpm install",
    "knip": "knip --include files,dependencies",
    "up": "docker compose -f ./deployment/docker-compose.yaml up",
    "migrate": "pnpm turbo run migrate"
  },
  "author": "Andreas Thomas",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
    "@changesets/cli": "^2.27.1",
    "@million/lint": "^0.0.73",
    "@types/node": "^20.14.9",
    "@vitest/ui": "^1.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "is-ci": "^3.0.1",
    "knip": "^5.11.0",
    "lint-staged": "^15.2.2",
    "ts-node": "latest",
    "turbo": "^2.1.3",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "pnpm": {
    "overrides": {
      "@opentelemetry/api": "1.4.1",
      "@opentelemetry/core": "1.13.0",
      "@opentelemetry/exporter-trace-otlp-grpc": "0.39.1",
      "@opentelemetry/resources": "1.13.0",
      "@opentelemetry/sdk-trace-base": "1.13.0",
      "@opentelemetry/sdk-trace-node": "1.13.0",
      "@opentelemetry/semantic-conventions": "1.13.0"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "pnpm@8.6.9+sha256.c94450b6623ecedb5e8c7045c7e67cb240bbe88e17660b3d8c57207dfd1eff90"
}
