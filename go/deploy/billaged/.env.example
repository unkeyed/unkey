# Billaged Environment Variables
# Copy this file to .env and adjust values as needed

# Service Configuration
UNKEY_BILLAGED_PORT=8081
UNKEY_BILLAGED_ADDRESS=0.0.0.0
UNKEY_BILLAGED_LOG_LEVEL=info
UNKEY_BILLAGED_AGGREGATION_INTERVAL=1m

# ClickHouse Configuration
UNKEY_BILLAGED_CLICKHOUSE_HOST=localhost
UNKEY_BILLAGED_CLICKHOUSE_PORT=9000
UNKEY_BILLAGED_CLICKHOUSE_DATABASE=unkey_billing
UNKEY_BILLAGED_CLICKHOUSE_USERNAME=default
UNKEY_BILLAGED_CLICKHOUSE_PASSWORD=
UNKEY_BILLAGED_CLICKHOUSE_TLS_ENABLED=false

# TLS Configuration
UNKEY_BILLAGED_TLS_MODE=spiffe
UNKEY_BILLAGED_SPIFFE_SOCKET=/run/spire/sockets/agent.sock

# OpenTelemetry Configuration
UNKEY_BILLAGED_OTEL_ENABLED=true
UNKEY_BILLAGED_OTEL_SERVICE_NAME=billaged
UNKEY_BILLAGED_OTEL_SERVICE_VERSION=0.1.0
UNKEY_BILLAGED_OTEL_SAMPLING_RATE=1.0
UNKEY_BILLAGED_OTEL_ENDPOINT=localhost:4318
UNKEY_BILLAGED_OTEL_PROMETHEUS_ENABLED=true
UNKEY_BILLAGED_OTEL_PROMETHEUS_PORT=9465